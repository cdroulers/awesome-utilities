<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <appSettings>
        <add key="Setting1" value="Setting1Value" />
        <add key="Setting2" value="Setting2Value" />
    </appSettings>

    <connectionStrings>
        <add name="String1" connectionString="Data Source=String1.s3db;" providerName="System.Data.SQLite"/>
    </connectionStrings>

    <system.data>
        <DbProviderFactories>
            <add name="SQLite Data Provider"
                 invariant="System.Data.SQLite"
                description=".Net Framework Data Provider for SQLite"
                type="System.Data.SQLite.SQLiteFactory, System.Data.SQLite"/>
            <add name="Npgsql PostgreSQL Provider"
                 invariant="System.Data.PostgreSQL"
                description=".Net Framework Data Provider for PostgreSQL"
                type="Npgsql.NpgsqlFactory, Npgsql"/>
        </DbProviderFactories>
    </system.data>
</configuration>